{
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "plugins": [{ "name": "ts-vue-plugin" }]
  },
  "include": ["./**/*.ts", "./**/*.vue"],
  "exclude": [
    "node_modules",
    "vendor"
  ],
  "compileOnSave": false
}
