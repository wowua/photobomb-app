@font-face {
  font-family: 'panton';
  src: url('../../assets/fonts/panton-bold-webfont.woff2') format('woff2'),
       url('../../assets/fonts/panton-bold-webfont.woff') format('woff');
  font-weight: normal;
  font-style: normal;
  font-display: optional;
}

@font-face {
  font-family: 'proxima';
